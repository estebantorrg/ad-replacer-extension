{
  "manifest_version": 3,
  "name": "Ad Replacer",
  "version": "1.1",
  "description": "Replaces ads on a webpage with a dog.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "dog.jpg" ],
      "matches": [ "<all_urls>" ]
    }
  ]
}